{
  "name": "AwesomeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest App.test.tsx",
    "test:reg:perf": "jest performance-regression.test.tsx",
    "test:all": "yarn test --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix",
    "e2e:android:build": "detox build --configuration android.emu.e2e.release",
    "e2e:android:test": "detox test --configuration android.emu.e2e.release",
    "e2e:android:test:update": "e2e:android:test && -updateSnapshot",
    "e2e:android": "yarn e2e:android:build && yarn e2e:android:test",
    "e2e:ios:build": "detox build --configuration ios.sim.debug",
    "e2e:ios:test": "detox test --configuration ios.sim.debug",
    "e2e:ios": "yarn e2e:ios:build && yarn e2e:ios:test"
  },
  "dependencies": {
    "@jest/globals": "^29.0.2",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.7.0",
    "@react-navigation/stack": "^6.2.3",
    "jest-image-snapshot": "^5.2.0",
    "react": "18.2.0",
    "react-native": "0.69.4",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-rename": "^2.9.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.1.0",
    "@testing-library/jest-native": "^4.0.11",
    "@testing-library/react-native": "^11.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^27.2.5",
    "@types/node": "^18.7.14",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^27.2.5",
    "detox": "^19.9.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^27.2.5",
    "metro-react-native-babel-preset": "^0.72.1",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "reassure": "^0.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "unset-value": "^2.0.1"
  }
}
